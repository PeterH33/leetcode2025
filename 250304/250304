def checkPowersOfThree(n: int) -> bool:
    powThree = [1,3,9,27,81,243,729,2187,6561,19683,59049,177147,531441,1594323,4782969]

    bits = 1
    for num in powThree:
        bits |= bits << num
        print(bin(bits))
    return bits >> n & 1

checkPowersOfThree(12)
checkPowersOfThree(91)
checkPowersOfThree(21)